{
  "name": "Motion",
  "url": "https://github.com/slu125/hassio-addons",
  "version": "0.4.29",
  "slug": "motion",
  "description": "Motion addon from LINUX package https://motion-project.github.io/",
  "startup": "before",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "audio": true,
  "devices": [
    "/dev/video0:/dev/video0:rwm"
  ],
  "hassio_api": true,
  "homeassistant_api": true,
  "privileged": [
    "SYS_ADMIN"
  ],
  "webui": "http://[HOST]:[PORT:7999]/",
  "timeout": 90,
  "tmpfs": "size=256m,uid=0,rw",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "7999/tcp": 7999,
    "8000/tcp": 8000,
    "8080/tcp": 8080,
    "8081/tcp": 8081,
    "8082/tcp": 8082,
    "8090/tcp": 8090,
    "8091/tcp": 8091,
    "8092/tcp": 8092,
    "8093/tcp": 8093,
    "8094/tcp": 8094,
    "8095/tcp": 8095,
    "8096/tcp": 8096,
    "8097/tcp": 8097,
    "8098/tcp": 8098,
    "8099/tcp": 8099,
    "8100/tcp": 8100,
    "8101/tcp": 8101,
    "8102/tcp": 8102,
    "8103/tcp": 8103,
    "8104/tcp": 8104,
    "8105/tcp": 8105,
    "8106/tcp": 8106,
    "8107/tcp": 8107,
    "8108/tcp": 8108,
    "8109/tcp": 8109,
    "8110/tcp": 8110,
    "8111/tcp": 8111,
    "8112/tcp": 8112,
    "8113/tcp": 8113,
    "8114/tcp": 8114,
    "8115/tcp": 8115,
    "8116/tcp": 8116,
    "8117/tcp": 8117,
    "8118/tcp": 8118,
    "8119/tcp": 8119
  },
  "options": {
    "log_level": "info",
    "log_motion": 2,
    "devicedb": "motion",
    "name": "hassio.local",
    "username": "",
    "password": "",
    "timezone": "Europe/Berlin",
    "unit_system": "metric",
    "cloudant": {
      "url": "",
      "username": "",
      "password": ""
    },
    "watson": {
      "url": "",
      "apikey": "",
      "classifier": "",
      "date": "",
      "version": ""
    },
    "digits": {
      "url": "",
      "jobid": ""
    },
    "mqtt": {
      "host": "core-mosquitto",
      "port": 1883
    },
    "cameras": [
      {
        "name": "sample",
        "device": "/dev/video0",
        "type": "ps3eye",
        "palette": 15,
        "fov": 75,
        "width": 640,
        "height": 480,
        "fps": 5
      }
    ]
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)",
    "devicedb": "str",
    "name": "str?",
    "elevation": "int?",
    "latitude": "float?",
    "longitude": "float?",
    "unit_system": "str?",
    "username": "str?",
    "password": "str?",
    "www": "str?",
    "port": "port?",
    "email": "email?",
    "timezone": "str?",
    "camera_type": "str?",
    "cameras": [
      {
        "name": "str",
        "device": "str?",
        "palette": "int?",
        "url": "str?",
        "mac": "str?",
        "type": "str?",
        "fov": "int?",
        "fps": "int?",
        "userpass": "str?",
        "keepalive": "match(^(on|off|force)$)?",
        "port": "port?",
        "quality": "int?",
        "width": "int?",
        "height": "int?",
        "rotate": "int?",
        "threshold": "int?",
        "event_gap": "int?",
        "brightness": "int?",
        "contrast": "int?",
        "saturation": "int?",
        "lightswitch": "int?",
        "models": "str?"
      }
    ],
    "webcontrol_port": "port?",
    "stream_port": "port?",
    "log_motion": "int?",
    "log_type": "str?",
    "videodevice": "str?",
    "v412_pallette": "int?",
    "auto_brightness": "str?",
    "locate_motion_mode": "str?",
    "locate_motion_style": "str?",
    "output_pictures": "str?",
    "picture_type": "str?",
    "threshold_tune": "str?",
    "pre_capture": "int?",
    "post_capture": "int?",
    "event_gap": "int?",
    "minimum_motion_frames": "int?",
    "quality": "int?",
    "width": "int?",
    "height": "int?",
    "framerate": "int?",
    "minimum_frame_time": "int?",
    "brightness": "int?",
    "contrast": "int?",
    "saturation": "int?",
    "hue": "int?",
    "rotate": "int?",
    "fov": "int?",
    "fps": "int?",
    "stream_quality": "int?",
    "threshold": "int?",
    "lightswitch": "int?",
    "netcam_url": "str?",
    "netcam_userpass": "str?",
    "netcam_keepalive": "match(^(on|off|force)$)?",
    "watson": {
      "url": "str?",
      "apikey": "str?",
      "classifier": "str?",
      "date": "str?",
      "version": "str?"
    },
    "digits": {
      "jobid": "str?",
      "url": "str?"
    },
    "mqtt": {
      "host": "str?",
      "port": "port?",
      "username": "str?",
      "password": "str?"
    },
    "cloudant": {
      "url": "str?",
      "username": "str?",
      "password": "str?"
    },
    "daemon": "str?",
    "interval": "int?",
    "post_pictures": "str?",
    "minimum_animate": "int?",
    "reload": "str?",
    "target_dir": "str?",
    "share_dir": "str?"
  },
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  }
}
